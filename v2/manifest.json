{
	"name": "PassMe",
	"version": "0.2",
	"description": "ETH Password Manager",
	"permissions": ["declarativeContent", "storage", "tabs", "http://*/*", "https://*/*"],
	"background": {
		"scripts": ["background.js", "popup.js"],
		"persistent": false
	},

	"browser_action":{
		"default_icon": {
			"32": "icons/lock32.png"
		}
		
	},
	
	"manifest_version": 2
}
